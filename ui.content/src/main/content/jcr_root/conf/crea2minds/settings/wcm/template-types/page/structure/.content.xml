<?xml version="1.0" encoding="UTF-8"?>
<jcr:root xmlns:sling="http://sling.apache.org/jcr/sling/1.0" xmlns:cq="http://www.day.com/jcr/cq/1.0" xmlns:jcr="http://www.jcp.org/jcr/1.0" xmlns:nt="http://www.jcp.org/jcr/nt/1.0"
    jcr:primaryType="cq:Page">
    <jcr:content
        cq:deviceGroups="[mobile/groups/responsive]"
        cq:lastReplicated="{Date}2023-03-13T14:11:49.134+05:30"
        cq:lastReplicated_publish="{Date}2023-03-13T14:11:49.134+05:30"
        cq:lastReplicatedBy="admin"
        cq:lastReplicatedBy_publish="admin"
        cq:lastReplicationAction="Deactivate"
        cq:lastReplicationAction_publish="Deactivate"
        cq:template="/conf/crea2minds/settings/wcm/templates/page-content"
        jcr:primaryType="cq:PageContent"
        sling:resourceType="crea2minds/components/page">
        <root
            jcr:primaryType="nt:unstructured"
            sling:resourceType="crea2minds/components/container"
            layout="responsiveGrid">
            <experiencefragment-header
                jcr:primaryType="nt:unstructured"
                sling:resourceType="crea2minds/components/experiencefragment"
                fragmentVariationPath="/content/experience-fragments/crea2minds/us/en/site/header/master"/>
